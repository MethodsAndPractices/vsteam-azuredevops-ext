{
  "name": "vsteam-azuredevops",
  "version": "0.1.0",
  "description": "Azure DevOps pipeline extension to run VSTeam PowerShell scripts natively and cross-platform",
  "scripts": {
    "test": " tsc && mocha vsteam-azuredevops/V1/tests/*.test.js",
    "testCovCobertura": " tsc && nyc --reporter=cobertura --reporter=text mocha 'vsteam-azuredevops/V1/tests/_suite.test.js'",
    "testCovHtml": " tsc && nyc --reporter=html --reporter=text mocha 'vsteam-azuredevops/V1/tests/_suite.test.js'"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/MethodsAndPractices/vsteam-azuredevops-ext.git"
  },
  "keywords": [
    "AzureDevOps",
    "DevOps",
    "PowerShell",
    "VSTeam",
    "TFS",
    "VSTS"
  ],
  "author": "Sebastian Sch√ºtze",
