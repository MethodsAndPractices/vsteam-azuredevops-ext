  "bugs": {
    "url": "https://github.com/MethodsAndPractices/vsteam-azuredevops-ext/issues"
  },
  "homepage": "https://github.com/MethodsAndPractices/vsteam-azuredevops-ext#readme",
  "dependencies": {},
  "devDependencies": {
    "@types/expect": "^24.3.0",
    "@types/mocha": "^8.0.0",
    "@types/node": "^14.0.23",
    "@types/q": "^1.5.4",
    "mocha": "^7.2.0",
    "nyc": "^15.1.0",
    "source-map-support": "^0.5.19",
    "sync-request": "^6.1.0",
    "typescript": "^3.9.7"
  }
